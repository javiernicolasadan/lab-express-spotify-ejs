<h2>Results</h2>
<% artists.forEach(eachArtist => { %>
<div class="each-artist">
  <h3><%- eachArtist.name %></h3>
  <br />
  <% if (eachArtist.images[0]) { %>
  <img src="<%- eachArtist.images[0].url %>" />
  <br />
  <a href="<%- `/albums/${eachArtist.id}` %>">
    <button>view albums</button>
  </a>
  <% } %>
</div>

<% }) %>
